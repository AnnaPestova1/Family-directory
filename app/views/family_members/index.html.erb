<p style="color: green"><%= notice %></p>

<h1>Family members</h1>

<div id="family_members" >
  <% @family_members.each do |family_member| %>
    <!-- <%= render family_member %> -->
    <div class="card" style="width: 18rem;">
  <!-- <img src="..." class="card-img-top" alt="..."> -->
  <div class="card-body">
    <h5 class="card-title"> <%= family_member.first_name %></h5>
    <p class="card-text"><%= family_member.relationship %>   <%= family_member.user_id %></p>
    <%= link_to "Show this family member", family_member, class: "btn btn-secondary" %>
  </div>
</div>
  <% end %>
</div>

<%= link_to "New family member", new_family_member_path, class: "btn btn-primary" %>
